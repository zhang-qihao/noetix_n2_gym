    <motor name='L_leg_hip_pitch'    	 joint='L_leg_hip_pitch_joint'/>
    <motor name='L_leg_knee'    	     joint='L_leg_knee_joint'/>
    <motor name='L_leg_ankle'    	     joint='L_leg_ankle_joint'/>

    <motor name='R_leg_hip_yaw'    	   joint='R_leg_hip_yaw_joint'/>
    <motor name='R_leg_hip_roll'    	 joint='R_leg_hip_roll_joint'/>
    <motor name='R_leg_hip_pitch'    	 joint='R_leg_hip_pitch_joint'/>
    <motor name='R_leg_knee'    	     joint='R_leg_knee_joint'/>
    <motor name='R_leg_ankle'    	     joint='R_leg_ankle_joint'/>
  </actuator>

  <sensor>
      <force name="L_leg_foot_force" site="L_leg_foot_contact_ground"/>
      <force name="R_leg_foot_force" site="R_leg_foot_contact_ground"/>
      <framequat     name='orientation' objtype='site' noise='0.001' objname='imu'/>
      <framepos      name='position' objtype='site' noise='0.001' objname='imu'/>
      <gyro          name='angular-velocity'    site='imu' noise='0.005' cutoff='34.9'/>
      <velocimeter   name='linear-velocity'     site='imu' noise='0.001' cutoff='30'/>
      <accelerometer name='linear-acceleration' site='imu' noise='0.005' cutoff='157'/>
      <magnetometer  name='magnetometer'        site='imu'/>
  </sensor>

